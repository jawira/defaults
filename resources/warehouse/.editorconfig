root = true

[*.php]
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.{yml,yaml,xml,neon,json}]
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true
